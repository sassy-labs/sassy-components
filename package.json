{
  "name": "sassy-components",
  "sideEffects": "false",
  "version": "0.1.6",
  "description": "A react ui components library inspired from shoelace",
  "license": "MIT",
  "author": {
    "name": "Siddharth Borderwala",
    "email": "siddharthborderwala@gmail.com",
    "url": "https://codexsid.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sassy-labs/sassy-components.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/sassy-components.esm.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build && cp ./src/components/icon/bootstrap-icons.svg ./dist/",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write src"
  },
  "peerDependencies": {
    "bootstrap-icons": ">=1.0.0",
    "react": ">=16.8",
    "styled-components": ">=3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@size-limit/preset-small-lib": "^4.6.0",
    "@storybook/addon-controls": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "commitizen": "^4.2.2",
    "husky": "^4.3.0",
    "prettier": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "size-limit": "^4.6.0",
    "styled-components": "^5.2.0",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/sassy-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sassy-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "bootstrap-icons": "^1.0.0",
    "styled-components": "^5.2.0"
  }
}
